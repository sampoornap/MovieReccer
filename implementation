      "source": [
        "def convert3(obj):\n",
        "  L=[]\n",
        "  counter=0\n",
        "  for i in ast.literal_eval(obj):\n",
        "    if counter!=3:\n",
        "      L.append(i[\"name\"])\n",
        "      counter+=1\n",
        "    else:\n",
        "      break  \n",
        "  return L"
      ]
    },
